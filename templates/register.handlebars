<div id="register">
    <div class="row">
        <div class="col-md-4">
            <h2 class="form-register-heading">Registration form</h2>
            <input type="text" placeholder="Username" class="form-control" id="tb-reg-username" />
            <input type="text" placeholder="First Name"  class="form-control" id="tb-first-name" />
            <input type="text" placeholder="Second Name"  class="form-control" id="tb-second-name" />
            <input type="password" placeholder="Password" class="form-control" id="tb-reg-pass" />
            <button class="btn btn-lg btn-primary btn-block" id="btn-register">Register</button>
        </div>
    </div>
</div>